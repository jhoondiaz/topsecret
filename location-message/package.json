{
    "name": "location-message",
    "version": "1.1.0",
    "main": "src/index.ts",
    "files": [
        "/src"
    ],
    "scripts": {
        "coverage": "nyc -r lcov -r text -e .ts -x \"test/*/.test.ts\" npm run test",
        "test": "jest --coverage",
        "prebuild": "rm -rf dist && cp package.json src/",
        "audit": "npm audit",
        "lint:fix": "eslint --fix .",
        "lint": "eslint .",
        "prettier:list": "prettier --list-different \"./{test,src}/**/*.js\" || ECHO .",
        "prettier:write": "prettier --write  \"./{test,src}/**/*.ts\"",
        "prettier": "prettier --check  \"./{test,src}/**/*.ts\"",
        "sonar": "sonar-scanner -Dsonar.organization=$SONAR_ORGANIZATION -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN",
        "build": "npm run clean && npm run build-app",
        "clean": "rimraf dist && rimraf coverage && rimraf location-message.zip",
        "build-app": "tsc -p tsconfig.json",
        "postbuild": "npm run copy-pck-files &&  npm run copy-mdl-files && npm run zip && mv location-message.zip dist/ && rm src/package.json",
        "copy-pck-files": "copyfiles -u 1 src/package.json dist/src ",
        "copy-mdl-files": "cp package.json dist && cd dist && npm i --production && cd ../",
        "zip": "cd dist/ && bestzip  ../location-message.zip *"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.9.1",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "aws-sdk-mock": "^5.6.2",
        "bestzip": "^2.1.7",
        "chai": "^4.2.0",
        "chai-string": "^1.5.0",
        "copyfiles": "^2.3.0",
        "eslint": "^7.32.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-prettier": "^3.1.0",
        "fs": "0.0.1-security",
        "jest": "^27.2.1",
        "jshint": "^2.10.2",
        "mocha": "^8.3.0",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "repack-zip": "^0.2.5",
        "sonar-scanner": "^3.1.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^8.10.1",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "aws-sdk": "^2.1038.0",
        "inversify": "^5.1.1",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.34",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2"
    }
}
